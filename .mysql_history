\040\040\040\040id\040SERIAL\040PRIMARY\040KEY,
\040\040\040\040name\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040last_name\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040created_at\040TIMESTAMP\040NOT\040NULL\040DEFAULT\040CURRENT_TIMESTAMP
);
CREATE\040TABLE\040clients\040(\040\040\040\040\040id\040SERIAL\040PRIMARY\040KEY,\040\040\040\040\040name\040VARCHAR(255)\040NOT\040NULL,\040\040\040\040\040last_name\040VARCHAR(255)\040NOT\040NULL,\040\040\040\040\040created_at\040TIMESTAMP\040NOT\040NULL\040DEFAULT\040CURRENT_TIMESTAMP\040);
/*pets\040table\040PostgreSQL*/
CREATE\040TABLE\040pets\040(
\040\040\040\040id\040SERIAL\040PRIMARY\040KEY,
\040\040\040\040name\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040type\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040breed\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040owner_id\040INTEGER\040REFERENCES\040clients(id)\040ON\040DELETE\040CASCADE,
\040\040\040\040created_at\040TIMESTAMP\040NOT\040NULL\040DEFAULT\040CURRENT_TIMESTAMP
);
CREATE\040TABLE\040pets\040(\040\040\040\040\040id\040SERIAL\040PRIMARY\040KEY,\040\040\040\040\040name\040VARCHAR(255)\040NOT\040NULL,\040\040\040\040\040type\040VARCHAR(255)\040NOT\040NULL,\040\040\040\040\040breed\040VARCHAR(255)\040NOT\040NULL,\040\040\040\040\040owner_id\040INTEGER\040REFERENCES\040clients(id)\040ON\040DELETE\040CASCADE,\040\040\040\040\040created_at\040TIMESTAMP\040NOT\040NULL\040DEFAULT\040CURRENT_TIMESTAMP\040);
/*appointments\040table\040PostgreSQL*/
CREATE\040TABLE\040appointments\040(
\040\040\040\040id\040SERIAL\040PRIMARY\040KEY,
\040\040\040\040pet_id\040INTEGER\040REFERENCES\040pets(id)\040ON\040DELETE\040CASCADE,
\040\040\040\040client_id\040INTEGER\040REFERENCES\040clients(id)\040ON\040DELETE\040CASCADE,
\040\040\040\040date\040TIMESTAMP\040NOT\040NULL,
\040\040\040\040created_at\040TIMESTAMP\040NOT\040NULL\040DEFAULT\040CURRENT_TIMESTAMP
);
